#: import FadeTransition kivy.uix.screenmanager.FadeTransition

	
<Home>:
	FloatLayout:
		MenuSpawner:
			timeout: .5
			menu_args:
				dict(
				creation_direction=-1,
				radius=100,
				creation_timeout=.4,
				choices=[
				dict(text='submenu 1', index=1, callback=app.callback1),
				dict(text='Calculator', index=2, callback=app.calculator),
				dict(text='Gallery', index=3, callback=app.pictures),
				dict(text='clock', index=4, callback=app.clock),
				dict(text='submenu 2', index=5, callback=app.callback4),
				dict(text='Exit', index=6, callback=app.callback5),
				])

	
