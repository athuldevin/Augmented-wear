#: import FadeTransition kivy.uix.screenmanager.FadeTransition

	
<Home>:
	FloatLayout:
		MenuSpawner:
			timeout: .5
			menu_args:
				dict(
				creation_direction=-1,
				radius=100,
				creation_timeout=.4,
				choices=[
				dict(text='Calculator', index=1, callback=app.calculator),
				dict(text='Gallery', index=2, callback=app.pictures),
				dict(text='Clock', index=3, callback=app.clock),
				dict(text='Paint', index=4, callback=app.paint),
				dict(text='Dev', index=5, callback=app.devolopers),
				dict(text='Camera', index=6, callback=app.camera),
				dict(text='Exit', index=7, callback=app.exit1),
				])

	
